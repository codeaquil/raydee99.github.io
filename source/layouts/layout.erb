  <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
  <title>raydee99.com | Video Game Music Composer</title>

  <link rel="canonical" href="https://www.raydee99.com/">
  <meta property="og:url" content="https://www.raydee99.com/">
  <meta property="og:type" content="website">
  <meta name="description" content="Meet Raydee99, a video game music composer and TTRPG creator! View his portfolio in music compositions, games, and podcasts.">
  <script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "WebSite",
      "name": "Raydee99's Portfolio",
      "url": "https://www.raydee99.com"
    }
  </script>

  <%= stylesheet_link_tag 'bootstrap_vapor_theme.css' %>
